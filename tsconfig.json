{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",                                
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "@utils/*": ["src/utils/*"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": ["./src/*.ts"],
  "exclude": ["node_modules", "dist"]
}