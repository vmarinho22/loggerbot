{
	"extends": [
		"eslint:recommended",
		"prettier",
		"plugin:prettier/recommended"
	],
	"plugins": ["prettier"],
	"env": {
		"node": true,
		"es6": true
	},
	"parserOptions": {
		"ecmaVersion": 2021
	},
	"rules":{
    "no-console": "off",
		"prettier/prettier": [
			"error",
			{
				"singleQuote": true,
				"parser": "flow"
			}
		]
	}
}